{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "." ,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@stores*": [
        "src/stores/*"
      ],
      "@viewmodels/*": [
        "src/viewmodels/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
    }
  }
}
